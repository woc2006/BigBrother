<%for(var i=0,len = rules.length;i<len;i++){
        var rule = rules[i];
        %>
<li><div id="ruleItem-<%=rule.id%>" class="checkbox <%=(rule.enable?'checkbox-on':'')%>"></div>
<span class="type"><%=rule.type%>:</span><span><%=rule.source%></span><span class="arrow">=&gt;</span><span><%=rule.dest%></span>
<div class="ruleItem-tool animate">
<img data-tool="copy" src="assets/img/copy.png"/>
<img data-tool="up" src="assets/img/up.png"/>
<img data-tool="down" src="assets/img/down.png"/>
<img data-tool="delete" src="assets/img/cancel.png"/>
</div></li>
<%}%>
